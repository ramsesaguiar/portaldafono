<h1>Listing admin_articles</h1>

<table>
  <tr>
    <th>Autor</th>
    <th>Titulo</th>
    <th>Subtitulo</th>
    <th>Texto</th>
    <th>Palavras chave</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_articles.each do |admin_article| %>
  <tr>
    <td><%= admin_article.autor_id %></td>
    <td><%= admin_article.titulo %></td>
    <td><%= admin_article.subtitulo %></td>
    <td><%= admin_article.texto %></td>
    <td><%= admin_article.palavras_chave %></td>
    <td><%= admin_article.data %></td>
    <td><%= link_to 'Show', admin_article %></td>
    <td><%= link_to 'Edit', edit_admin_article_path(admin_article) %></td>
    <td><%= link_to 'Destroy', admin_article, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

